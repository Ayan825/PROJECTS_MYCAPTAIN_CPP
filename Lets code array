#include <iostream>
using namespace std;

int main()
{
    float arr[10],n;
    
    cout<<"enter the number of elements: ";
    cin>>n;
    
    cout<<"Enter the elements: "<<endl;
    for(int i = 0; i<n; i++)
    {
        cin>>arr[i];
    }
    cout<<"You entered:"<<endl;
    for(int i = 0; i < n; ++i)
    {
        cout << endl << *(arr + i);
    }    
    return 0;
}
